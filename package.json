{
  "name": "btimer",
  "productName": "BTimer",
  "version": "1.0.0",
  "description": "A simple timer for web, mobile and desktop.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/BrOrlandi/BTimer.git"
  },
  "author": "BrOrlandi",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "npm run dev",
    "watch": "parcel watch ./src/index.html -d dist --public-url ./",
    "build": "rm -rf dist && parcel build ./src/index.html -d dist --public-url ./ ",
    "dev": "parcel serve ./src/index.html -o -d dist",
    "lint": "eslint ./src",
    "deploy": "npm run build-web && ghpages -p dist"
  },
  "dependencies": {
    "moment": "^2.19.3",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ghpages": "^0.0.10",
    "parcel-bundler": "^1.5.0",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5"
  },
  "bugs": {
    "url": "https://github.com/BrOrlandi/BTimer/issues"
  },
  "homepage": "https://github.com/BrOrlandi/BTimer#readme"
}
